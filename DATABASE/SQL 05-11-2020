from student
where major = 'biology';

+------+---------+
| name | major   |
+------+---------+
| jack | biology |
| jack | biology |
+------+---------+
_____________________________________________________________________________________

select name,major
from student
where major = 'biology' OR major = 'chemistry';

+--------+-----------+
| name   | major     |
+--------+-----------+
| jack   | biology   |
| claire | chemistry |
| jack   | biology   |
+--------+-----------+
_____________________________________________________________________________________

select name,major
from student
where major = 'biology' OR name = 'kate';

+------+-----------+
| name | major     |
+------+-----------+
| jack | biology   |
| kate | sociology |
| jack | biology   |
+------+-----------+
_____________________________________________________________________________________
=====================================================================================

                               <<<<<( <> , < , > , <= , >= )>>>>>
select *
from student
where major <> 'biology';

+------------+--------+------------------+
| student_id | name   | major            |
+------------+--------+------------------+
|          2 | kate   | sociology        |
|          3 | claire | chemistry        |
|          5 | mike   | computer science |
+------------+--------+------------------+
_____________________________________________________________________________________

select *
from student
where student_id < 3;

+------------+------+-----------+
| student_id | name | major     |
+------------+------+-----------+
|          1 | jack | biology   |
|          2 | kate | sociology |
+------------+------+-----------+
_____________________________________________________________________________________

select *
from student
where student_id <= 3;

+------------+--------+-----------+
| student_id | name   | major     |
+------------+--------+-----------+
|          1 | jack   | biology   |
|          2 | kate   | sociology |
|          3 | claire | chemistry |
+------------+--------+-----------+
_____________________________________________________________________________________

select *
from student
where student_id <= 3 AND name <> 'jack';

+------------+--------+-----------+
| student_id | name   | major     |
+------------+--------+-----------+
|          2 | kate   | sociology |
|          3 | claire | chemistry |
+------------+--------+-----------+
_____________________________________________________________________________________

select *
from student
where name IN('claire','kate','mike');

+------------+--------+------------------+
| student_id | name   | major            |
+------------+--------+------------------+
|          2 | kate   | sociology        |
|          3 | claire | chemistry        |
|          5 | mike   | computer science |
