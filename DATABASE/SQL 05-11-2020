from student
ORDER BY major, student_id DESC;

+------------+--------+------------------+
| student_id | name   | major            |
+------------+--------+------------------+
|          4 | jack   | biology          |<---
|          1 | jack   | biology          |<---
|          3 | claire | chemistry        |
|          5 | mike   | computer science |
